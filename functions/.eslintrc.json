{
  "root": true,
  "env": {
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "ignorePatterns": [
    "/lib/**/*",
    "/generated/**/*"
  ],
  "rules": {
    "quotes": ["error", "double"],
    "indent": ["error", 2],
    "no-unused-vars": "off",
    "no-explicit-any": "off"
  }
}
